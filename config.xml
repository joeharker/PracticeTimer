<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0"
		id="com.Joe007dev.Practice_Timer"
		version="2.0.0"
		android-versionCode="20000">
  <!--id="your.bundle.identifier" -->
  <!--sku=a number you keep track of for each app=0001 -->

  <name>Practice Timer</name>
  <description>Practice drills for live fire and dry practice pistol presenting from the holster</description>
  <author href="http://007dev.com" email="joeharker@007dev.com">Joe Harker</author>
	
  <content src="index.html"/>

	<preference name="permissions" value="none" />
	<preference name="orientation" value="portrait" />
	<preference name="target-device" value="universal" />
	<preference name="fullscreen" value="false" />
	<preference name="webviewbounce" value="false" />
	<preference name="stay-in-webview" value="false" />
	<preference name="ios-statusbarstyle" value="black-opaque" />
	<preference name="detect-data-types" value="true" />
	<preference name="exit-on-suspend" value="false" />
	<preference name="disable-cursor" value="false" />
	<preference name="android-minSdkVersion" value="14" />
	<preference name="android-installLocation" value="auto" />
	<preference name="SplashScreenDelay" value="0" />
	<preference name="show-splash-screen-spinner" value="false" />

